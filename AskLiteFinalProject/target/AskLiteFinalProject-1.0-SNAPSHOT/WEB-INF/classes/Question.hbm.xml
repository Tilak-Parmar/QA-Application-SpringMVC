<hibernate-mapping>
  <class name="com.asklite.pojo.Question" table="Ques">
    <id name="ID" column="QueID" type="int">
      <generator class="native"/>
    </id>
    <property name="question" column="Question" type="string"/>
    <property name="userID" column="UserID" type="int"/>
    <property name="approved" column="Approved" type="boolean"/>
  </class>
</hibernate-mapping>